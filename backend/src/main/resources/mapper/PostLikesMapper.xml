<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.lujs.community.service.mapper.PostLikesMapper">

    <select id="getByPostId" resultType="com.github.lujs.community.api.model.pojo.PostLikes">
        select * from post_likes
        where post_id = #{id}
    </select>
</mapper>
